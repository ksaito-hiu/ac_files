<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8">
    <title>index_of <%= c_path %></title>
  </head>
  <body>
        <h1><%= c_path %></h1>
        <ul>
          <% files.forEach(function(file) { %>
            <% if (file.isDirectory()) { %>
              <li><a href="./<%=(file.name+'/') %>"><%=file.name %></a></li>
            <% } else { %>
              <li><a href="./<%=file.name%>"><%=file.name %></a></li>
            <% } %>
          <% }); %>
        </ul>
  </body>
</html>
